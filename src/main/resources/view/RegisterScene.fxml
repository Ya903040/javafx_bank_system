<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" style="-fx-background-color: E6BA95;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.java.controllers.RegisterSceneController">
   <TextField fx:id="firstNameTextField" layoutX="251.0" layoutY="92.0" prefHeight="25.0" prefWidth="220.0"/>
   <TextField fx:id="lastNameTextField" layoutX="251.0" layoutY="153.0" prefHeight="25.0" prefWidth="220.0"/>
   <TextField fx:id="emailTextField" layoutX="251.0" layoutY="215.0" prefHeight="25.0" prefWidth="220.0"/>
   <PasswordField fx:id="passwordField" layoutX="251.0" layoutY="274.0" prefHeight="25.0" prefWidth="220.0"/>
   <Button fx:id="backButton" layoutX="130.0" layoutY="352.0" mnemonicParsing="false" onAction="#switchToLogInScene"
           prefHeight="35.0" prefWidth="124.0" text="Back" textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Button>
   <Button fx:id="registerButton" layoutX="347.0" layoutY="352.0" mnemonicParsing="false" onAction="#register"
           prefHeight="35.0" prefWidth="124.0" text="Register" textFill="RED">
      <font>
         <Font size="15.0"/>
      </font>
   </Button>
   <Label layoutX="130.0" layoutY="84.0" text="First Name" textFill="RED">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <Label layoutX="130.0" layoutY="148.0" text="Last Name" textFill="RED">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <Label layoutX="157.0" layoutY="210.0" text="Email" textFill="RED">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <Label layoutX="130.0" layoutY="269.0" text="Password" textFill="RED">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <Label fx:id="errorLabel" alignment="CENTER" layoutY="317.0" prefHeight="19.0" prefWidth="600.0" textFill="RED">
      <font>
         <Font size="13.0"/>
      </font>
   </Label>
</AnchorPane>
